{
    "name": "bupy7/zf-form",
    "description": "Simple form for Zend Framework 3.",
    "license": "BSD 3-Clause",
    "keywoards": [
        "zend",
        "form",
        "zf3",
        "zend framework",
        "zend framework 3"
    ],
    "authors": [
        {
            "name": "Vasilij Belosludcev",
            "email": "bupy765@gmail.com"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "adamwathan/form": "~0.8.9",
        "zendframework/zend-view": "^2.8",
        "zendframework/zend-modulemanager": "^2.7.2",
        "zendframework/zend-servicemanager": "^3.1",
        "zendframework/zend-validator": "^2.8",
        "zendframework/zend-inputfilter": "^2.7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5",
        "satooshi/php-coveralls": "1.0.*",
        "zendframework/zend-test": "^3.0.1"
    },
    "autoload": {
        "psr-4": {
            "Bupy7\\Form\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bupy7\\Form\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "phpunit": "php ./vendor/bin/phpunit --debug"
    }
}
