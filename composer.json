{
    "name": "bupy7/zf-form",
    "description": "Simple form for Zend Framework 3.",
    "version": "1.2.2",
    "license": "BSD-3-Clause",
    "keywoards": [
        "zend",
        "form",
        "zf",
        "zf3",
        "zend framework",
        "zend framework 3"
    ],
    "authors": [
        {
            "name": "Vasilij Belosludcev",
            "email": "bupy765@gmail.com"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0 < 7.2",
        "adamwathan/form": "~0.8.9",
        "zendframework/zend-view": "^2.8",
        "zendframework/zend-modulemanager": "^2.7.2",
        "zendframework/zend-servicemanager": "^3.1",
        "zendframework/zend-validator": "^2.8",
        "zendframework/zend-inputfilter": "^2.7.2"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.5 <6.0.0",
        "satooshi/php-coveralls": "^1.0.1",
        "zendframework/zend-test": "^3.0.1",
        "friendsofphp/php-cs-fixer": "~2.2.3"
    },
    "autoload": {
        "psr-4": {
            "Bupy7\\Form\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bupy7\\Form\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --debug",
        "fix": "./vendor/bin/php-cs-fixer fix --config=./phpcs.php",
        "clean": "bash ./bin/clean"
    }
}
