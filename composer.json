{
    "name": "bupy7/zf-form",
    "description": "Simple form for Zend Framework 3.",
    "license": "BSD 3-Clause",
    "keywoards": [
        "zend",
        "form",
        "zf3",
        "zend framework",
        "zend framework 3"
    ],
    "authors": [
        {
            "name": "Vasilij Belosludcev",
            "email": "bupy765@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "adamwathan/form": "~0.8.9",
        "zendframework/zend-view": "^2.5",
        "zendframework/zend-modulemanager": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "satooshi/php-coveralls": "1.0.*"
    },
    "autoload": {
        "psr-4": {
            "Bupy7\\Form\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bupy7\\Form\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "phpunit": "php ./vendor/bin/phpunit"
    }
}
